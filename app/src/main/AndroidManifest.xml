<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reza.form">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        //LOGIN ADMIN ACTIVITY
        <activity
            android:name=".LoginAdminActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        //LOGIN USER ACTIVITY
        <activity
            android:name=".LoginUserActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        //TANGGAL SURVEY AREA
        <activity
            android:name=".TanggalSurveyArea"
            android:label="@string/Tanggal_Survey" />
        <activity
            android:name=".InfoKabKota"
            android:label="@string/Info_Kab_Kota"/>
        <activity android:name=".InformasiDetailSurveyArea"
            android:label="@string/Info_detail_survey"/>
    </application>

</manifest>